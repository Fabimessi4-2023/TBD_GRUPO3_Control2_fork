# Database Configuration
DB_URL=jdbc:postgresql://localhost:5432/tbd_db
DB_USER=postgres
DB_PASS=postgres

# JWT Configuration
# Generate a secure secret key (at least 32 characters for HS256)
JWT_SECRET=your-very-secret-key-here-minimum-32-characters-for-security
# Token expiration: 86400000 = 1 day in milliseconds
JWT_EXPIRATION=86400000
# Refresh token expiration: 2592000000 = 30 days in milliseconds
JWT_REFRESH_EXPIRATION=2592000000

# CORS Configuration
# Comma-separated list of allowed origins
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8081
